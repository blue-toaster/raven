{
  "name": "raven",
  "version": "2.13.1",
  "author": "ArtieFuzzz#8298",
  "packageManager": "yarn@3.2.1",
  "main": "./build/main.js",
  "scripts": {
    "lint": "eslint --cache --report-unused-disable-directives --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "eslint --cache --report-unused-disable-directives --ext .ts src/ --fix",
    "pre:build": "node scripts/clean.mjs",
    "build": "yarn pre:build && tsc -p src",
    "watch": "tsc --watch -p src",
    "start": "prisma migrate deploy; node ./build/main.js",
    "dev": "docker compose up -d influx redis postgres && run-p watch start",
    "migrate:start": "docker compose up -d postgres; prisma migrate dev --name raven",
    "migrate": "prisma migrate dev --name raven",
    "prism": "prisma generate"
  },
  "devDependencies": {
    "@artiefuzzz/eslint-config": "1.3.1",
    "@artiefuzzz/ts": "1.3.2",
    "@types/node": "17.0.9",
    "eslint": "8.18.0",
    "npm-run-all": "4.1.5",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@artiefuzzz/lynx": "^2.2.5",
    "@artiefuzzz/utils": "^1.2.4",
    "@influxdata/influxdb-client": "^1.24.0",
    "@prisma/client": "latest",
    "@sapphire/decorators": "^4.0.0",
    "@sapphire/discord.js-utilities": "^4.4.0",
    "@sapphire/framework": "^2.3.0",
    "@sapphire/pieces": "^3.3.0",
    "@sapphire/plugin-editable-commands": "^1.1.3",
    "@sapphire/plugin-hmr": "^1.1.0",
    "@sapphire/plugin-logger": "^2.1.2",
    "@sapphire/plugin-scheduled-tasks": "^3.0.0",
    "@sapphire/plugin-subcommands": "^2.1.3",
    "@sapphire/ratelimits": "^2.3.1",
    "@sapphire/stopwatch": "^1.4.0",
    "@sapphire/time-utilities": "^1.5.1",
    "@sapphire/type": "^2.1.2",
    "@sapphire/utilities": "^3.2.0",
    "bufferutil": "^4.0.6",
    "bull": "^4.8.1",
    "colorette": "^2.0.16",
    "discord.js": "^13.6.0",
    "dotenv-cra": "^3.0.2",
    "erlpack": "^0.1.4",
    "fs-extra": "^10.0.1",
    "prisma": "^3.8.1",
    "reflect-metadata": "^0.1.13",
    "utf-8-validate": "^5.0.9",
    "zlib-sync": "^0.1.7"
  },
  "imports": {
    "#lib/*": "./build/lib/*.js",
    "#lib/env": "./build/lib/env/index.js",
    "#types": "./build/lib/@types/index.js",
    "#util": "./build/lib/util/index.js",
    "#util/*": "./build/lib/util/*.js"
  }
}
